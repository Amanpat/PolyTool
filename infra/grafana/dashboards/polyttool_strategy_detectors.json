          },
          "queryType": "timeseries",
          "rawSql": "SELECT\n  bucket_start AS time,\n  multiIf(\n    detector_name = 'HOLDING_STYLE', 'Holding Style',\n    detector_name = 'DCA_LADDERING', 'DCA / Laddering',\n    detector_name = 'MARKET_SELECTION_BIAS', 'Market Concentration',\n    detector_name = 'COMPLETE_SET_ARBISH', 'Complete-Set Arb',\n    detector_name\n  ) AS detector,\n  argMax(score, computed_at) AS score\nFROM polyttool.detector_results\nWHERE proxy_wallet = '$proxy_wallet'\n  AND bucket_type = '$bucket_type'\nGROUP BY bucket_start, detector_name\nORDER BY time",
          "refId": "A"
        }
      ],
      "title": "Detector Scores Over Time",
      "type": "timeseries"
    },
